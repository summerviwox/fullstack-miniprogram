<cover-view class="tabbar">
   <cover-view class="tabbar-border"></cover-view>
    <cover-view class="tabbar-item" data-page="{{item.page}}" wx:for="{{list}}" wx:key="index" data-index="{{index}}" bindtap="switchTab">
        <cover-image class="tabbar-image" src="{{selected==index?item.selectIcon:item.tabBarIcon}}"></cover-image>
    </cover-view>
</cover-view>