<page-meta>
  <navigation-bar
   title="{{titles[position].title}}"
    front-color="#ffffff"
    background-color="#2b2b2b"
  />
</page-meta>
<view class="root">
  <view class="midcontainer">
    <home wx:if="{{position==titles[0].index}}"></home>
    <second wx:if="{{position==titles[1].index}}"></second>
    <mine wx:if="{{position==titles[2].index}}"></mine>
  </view>
  <bottomview class="bottomview" bindonTabSelect="onTabSelect"></bottomview>
</view>
